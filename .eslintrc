{
    "rules": {
        'eqeqeq': 2,
            	'require-jsdoc': 2,
            	'brace-style': [2, '1tbs'],
            	'max-len': [2, 120],
            	'no-spaced-func': 2,
            	'camelcase': 1,
            	'no-trailing-spaces': 2,
            	'quotes': [2, 'single'],
            	'semi': 2,
            	'strict': 2,
            	'indent': [2, 2],
            	'array-bracket-spacing': 2,
            	'operator-linebreak': [2, 'before'],
            	'comma-dangle': [1, 'never'],
            	'one-var': [2, 'never'],
              'no-var': 2
    }
}